datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
  binaryTargets   = ["debian-openssl-1.1.x", "linux-musl-openssl-3.0.x", "darwin", "darwin-arm64", "debian-openssl-3.0.x", "windows"]
}
